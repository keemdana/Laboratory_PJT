<mapper namespace="com.vertexid.viself.hr.UserLogin.mssql">
    <select id="login" parameterType="paramMap" resultType="paramMap" fetchSize="1000">
			select
				*
			from v_sys_user
			where login_id = #{loginId}
     </select>
</mapper>